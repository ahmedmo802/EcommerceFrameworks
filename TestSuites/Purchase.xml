<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite parallel="tests" name="Suite">
  <groups>
		<run>
			<include name="Purches"></include>
		</run>
	</groups>
	<listeners>
		<listener class-name="EcommeraceFrameworks.TestComponents.Listeners"></listener>
	</listeners>
  <test thread-count="5" name="submitOrder">
    <classes>
      <class name="EcommeraceFrameworks.Tests.SubmitOrderTest"/>
  </classes>
  </test> <!-- Test -->
  <test thread-count="5" name="errorValidation">
    <classes>
      <class name="EcommeraceFrameworks.Tests.ErrorValidationTest"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
